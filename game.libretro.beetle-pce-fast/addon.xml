<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.beetle-pce-fast"
		name="PC Engine (Beetle PCE Fast)"
		version="0.9.36"
		provider-name="Libretro">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
                   library_android="libgame.libretro.beetle-pce-fast.so"
                   library_linux="game.libretro.beetle-pce-fast.so"
                   library_osx="game.libretro.beetle-pce-fast.dylib"
                   library_windx="game.libretro.beetle-pce-fast.dll"
                   library_wingl="game.libretro.beetla-pce-fast.dll">
		<platforms>PCE</platforms>
		<extensions>pce|cue|ccd</extensions>
		<supports_vfs>true</supports_vfs>
		<supports_no_game>false</supports_no_game>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Beetle PCE Fast v0.9.36.1</summary>
		<description>Beetle PCE Fast, a PC Engine emulator for Libretro, is a fork of Mednafen's PC Engine driver.

Supported files: .pce, .cue, .ccd

License: GPLv2</description>
		<platform>all</platform>
		<nofanart>true</nofanart>
	</extension>
</addon>
